# Supabase 설정
SUPABASE_URL=your-supabase-url-here
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_KEY=your-service-role-key-here

# 카카오 OAuth 설정
KAKAO_JS_KEY=your-kakao-javascript-key-here
KAKAO_REST_API_KEY=your-kakao-rest-api-key-here
KAKAO_CLIENT_SECRET=your-kakao-client-secret-here

# OpenAI API
OPENAI_API_KEY=your-openai-api-key-here

# 서버 설정
PORT=4000
NODE_ENV=development
FRONTEND_URL=http://localhost:3000

# CORS 설정 (선택사항, 쉼표로 구분)
# CORS_ORIGINS=http://localhost:3000,http://localhost:3001

# 로깅 레벨 (선택사항)
# LOG_LEVEL=info

# Rate Limiting 설정 (선택사항, 기본값 사용 시 생략 가능)
# RATE_LIMIT_WINDOW_MS=900000
# RATE_LIMIT_MAX=100
# AUTH_RATE_LIMIT_WINDOW_MS=900000
# AUTH_RATE_LIMIT_MAX=5
# AI_RATE_LIMIT_WINDOW_MS=3600000
# AI_RATE_LIMIT_MAX=20
# ADMIN_RATE_LIMIT_WINDOW_MS=900000
# ADMIN_RATE_LIMIT_MAX=50

